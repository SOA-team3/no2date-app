<% if flash[:error] %>
  <div class="flex flex-col items-center justify-center alert alert-danger "  id="flash_bar_danger">
    <%= flash[:error]%>
  </div>
<% end %>
<% if flash[:notice] %>
  <div class="flex flex-col items-center justify-center alert alert-success"  id="flash_bar_success">
    <%= flash[:notice]%>
  </div>
<% end %>
